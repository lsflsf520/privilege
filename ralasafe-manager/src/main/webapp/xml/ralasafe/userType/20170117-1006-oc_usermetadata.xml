<?xml version="1.0"?>
<user>
	<table ds="user" name="mainTable" sqlName="user_user"
		uniqueFields="name">
		<field name="id" columnName="id" sqlType="int" javaType="java.lang.Integer" />
		<field name="name" columnName="name" sqlType="varchar(40)"
			javaType="java.lang.String" displayName="enName" show="true" />
		<field name="cnName" columnName="cn_name" sqlType="varchar(100)"
			javaType="java.lang.String" displayName="CnName" show="true" />
		<field name="password" columnName="password" sqlType="varchar(40)"
			javaType="java.lang.String" />
		<field name="email" columnName="email" sqlType="varchar(64)"
			javaType="java.lang.String" />
		<field name="cellphone" columnName="cellphone" sqlType="varchar(64)"
			   javaType="java.lang.String" />
	</table>
</user>

